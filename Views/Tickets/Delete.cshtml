@model Primera.Models.Ticket

@{
    ViewData["Title"] = "Eliminar Ticket";
}

<h4>Ticket a eliminar</h4>
<hr />
<dl class="row">
    <dt class="col-sm-2">Placa</dt>
    <dd class="col-sm-10">@Model.Vehiculo?.NoPlaca</dd>

    <dt class="col-sm-2">Espacio</dt>
    <dd class="col-sm-10">@Model.EspacioEstacionamiento?.Estado</dd>

    <dt class="col-sm-2">Fecha de entrada</dt>
    <dd class="col-sm-10">@Model.Fecha_hora_entrada</dd>

    <dt class="col-sm-2">Tarifa</dt>
    <dd class="col-sm-10">@Model.Tarifa?.Descripcion</dd>

    <dt class="col-sm-2">Estado</dt>
    <dd class="col-sm-10">@Model.Estado</dd>


</dl>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id_Ticket" />
    <input type="submit" value="Eliminar" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
{}